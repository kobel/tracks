<div class="list-stategroup-contexts-container">
  <h2><span id="<%= state %>-contexts-count" class="badge"><%= context_state_group.length %></span><%= state.titlecase %> <%= I18n.t("common.Contexts") %></h2>
  <div id="<%= state%>-contexts-empty-nd" style="<%= no_contexts ? 'display:block' : 'display:none'%>">
    <div class="message"><p><%= I18n.t("common.currently_there_are_no") %> <%= state %> <%= I18n.t("common.contexts") %></p></div>
  </div>
  <div id="list-contexts-<%= state %>">
    <%= render :partial => 'context_listing', :collection => context_state_group %>
  </div>
</div>
